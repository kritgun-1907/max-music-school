{
  "name": "max-music-school",
  "version": "1.0.0",
  "private": true,
  "description": "Music education management platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "setup": "npm install && npm run setup:env",
    "setup:env": "node scripts/setup-env.js",
    "backend": "npm run dev --workspace=apps/backend",
    "web": "npm run dev --workspace=apps/web",
    "mobile": "npm run dev --workspace=apps/mobile",
    "test:api": "node scripts/test-api.js",
    "seed": "node scripts/seed-data.js",
    "backup": "node scripts/backup-sheets.js"
  },
  
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/connect": "^3.4.38",
    "@types/express": "^5.0.5",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/http-errors": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.19.24",
    "@types/prop-types": "^15.7.15",
    "@types/qs": "^6.14.0",
    "@types/range-parser": "^1.2.7",
    "@types/send": "^1.2.1",
    "@types/serve-static": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "turbo": "^1.10.15",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/max-music-school.git"
  },
  "keywords": [
    "music",
    "education",
    "school",
    "management",
    "react-native",
    "nextjs",
    "nodejs"
  ],
  "author": "Max Music School Team",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cssesc": "^3.0.0",
    "postcss": "^8.5.6"
  }
}
